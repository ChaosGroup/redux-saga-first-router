{
	"plugins": ["jest"],
	"extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
	"env": {
		"browser": true,
		"es6": true,
		"jest/globals": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"classes": true,
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"ecmaVersion": 2017,
		"sourceType": "module"
	}
}
